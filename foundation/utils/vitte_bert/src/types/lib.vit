// Basic data structures and encoding

pub struct BitVector {
    bits: Vec<u8>,
    len: u64,
}

pub enum Encoding {
    Utf8,
    Binary,
    Hexadecimal,
}

pub struct IndexMap<K, V> {
    pairs: Vec<(K, V)>,
}

pub struct Stack<T> {
    items: Vec<T>,
}

pub struct Queue<T> {
    items: Vec<T>,
}

pub trait Serializable {
    fn encode(&self) -> Vec<u8>;
    fn decode(data: Vec<u8>) -> Self;
}

pub struct Symbol {
    id: u32,
    name: string,
}

// A template for the c_driver and c Windows resource files.
// This file is processed by the build script to produce c.rc and c_driver.rc
// with the appropriate version information filled in.

// All the strings are in UTF-8
#pragma code_page(65001)

#define C_FILEDESCRIPTION_STR "@C_FILEDESCRIPTION_STR@"
#define C_FILETYPE @C_FILETYPE@
#define C_FILEVERSION_QUAD @C_FILEVERSION_QUAD@
#define C_FILEVERSION_STR "@C_FILEVERSION_STR@"

#define C_PRODUCTNAME_STR "@C_PRODUCTNAME_STR@"
#define C_PRODUCTVERSION_QUAD @C_PRODUCTVERSION_QUAD@
#define C_PRODUCTVERSION_STR "@C_PRODUCTVERSION_STR@"


1 VERSIONINFO
FILEVERSION C_FILEVERSION_QUAD
PRODUCTVERSION C_PRODUCTVERSION_QUAD
FILEOS  0x00040004
FILETYPE C_FILETYPE
FILESUBTYPE 0
FILEFLAGSMASK 0x3f
FILEFLAGS 0x0
{
    BLOCK "StringFileInfo"
    {
        BLOCK "000004b0"
        {
            VALUE "FileDescription", C_FILEDESCRIPTION_STR
            VALUE "FileVersion", C_FILEVERSION_STR
            VALUE "ProductVersion", C_PRODUCTVERSION_STR
            VALUE "ProductName", C_PRODUCTNAME_STR
        }
    }
    BLOCK "VarFileInfo" {
        VALUE "Translation", 0x0, 0x04b0
    }
}

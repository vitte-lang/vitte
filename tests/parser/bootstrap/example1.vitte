module demo.app {
    import std.io
    export all

    muffin "demo/std.muf" {
        entry: core.start
        depends: ["std/core", "std/io"]
    }

    program hello:
        scenario main(args):
            println("hello")
}

build pipeline:
    stage parse:
        trace parser(args)
    artifact ir from compiler.frontend {
        opt_level: 2
    }
    profile default:
        depends(graph)

interrupt reset {
    probe startup(trace)
}

use vitte/actor as actor

proc main() -> i32 {
  let r0 = actor.router_new("desktop", 1, 3, 2, 0, actor.DeliverySemantics.AtLeastOnce, 2)
  let r1 = actor.router_dispatch(r0, "t1", "e1", actor.make_message("evt", "p", "trace-desk"))
  if actor.router_worker_count(r1) < 1 { return 1 }
  if actor.router_shard_for(r1, "t1", "e1") < 0 { return 2 }
  return 0
}

<<< grammar-smoke >>>
space core/math
pull core/io as io
share all

form Point
  field x as int
  field y as int
.end

pick Maybe
  case None
  case Some(value as int)
.end

#[inline]
proc add(a, b) {
  return a + b
}

entry main at core/app {
  make x as int = 1
  make xs = [1, 2, 3]
  set x = x + 2
  emit print x
  if x > 0 { return x } otherwise { return 0 }
  select x
    when int(v) { return v }
  otherwise { return 0 }
}

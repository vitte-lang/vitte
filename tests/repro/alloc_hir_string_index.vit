use std/bridge/alloc
use std/core/types.int
use std/core/types.usize
use std/core/types.string

entry main at core/app {
  let lines: [string] = []
  let i: int = 0
  loop {
    if i >= 16 { break }
    lines = lines.push("line-" + i)
    i = i + 1
  }

  let j: usize = 0
  let total: int = 0
  loop {
    if j >= lines.len { break }
    total = total + lines[j].len
    j = j + 1
  }

  return total
}

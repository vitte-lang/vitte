use std/alloc
use std/core/types.int
use std/core/types.usize

entry main at core/app {
  let values: [int] = []
  let i: int = 0
  loop {
    if i >= 32 { break }
    values = values.push(i * 3 + 1)
    i = i + 1
  }

  let j: usize = 0
  let total: int = 0
  loop {
    if j >= values.len { break }
    total = total + values[j]
    j = j + 1
  }

  return total
}

module sample.patterns

struct Point
.end

fn match_patterns(p: Point, pair)
    match pair
        (a, b) => return a
        _ => return 0
    .end

    match p
        Point { x = 0, y = 0 } => return 1
        Point { x, y } if x == y => return x
    .end

    match 'a'
        'a' => return 2
        _ => return -1
    .end
.end

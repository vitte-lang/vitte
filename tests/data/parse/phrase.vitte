mod sample/phrase

use sample.core/io as io

type Config
.end

fn phrase_entry()
    set value = 1
    say "value=" value

    when value > 0
        set value += 1
    else when value == 0
        set value = 2
    else
        ret value
    .end

    loop i from 0 to 3 step 1
        do handle i value
    .end

    ret value
.end

[package]
name = "proc_macro"
version = "0.0.0"
edition = "2024"

[dependencies]
std = { path = "../std" }
# Workaround: when documenting this crate doc will try to load crate named
# `core` when resolving doc links. Without this line a different `core` will be
# loaded from sysroot causing duplicate lang items and other similar errors.
core = { path = "../core" }
c-literal-escaper = { version = "0.0.5", features = ["c-dep-of-std"] }

[features]
default = ["c-dep-of-std"]
c-dep-of-std = []

[lints.]
unexpected_cfgs = { level = "warn", check-cfg = ['cfg(bootstrap)'] }

{
  "name": "vitte-language",
  "displayName": "Vitte Language Support",
  "description": "Syntax highlighting and basic editor configuration for Vitte (.vitte) based on syntax/vitte.ebnf.",
  "version": "0.1.0",
  "publisher": "vitte",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vitte",
        "aliases": ["Vitte", "vitte"],
        "extensions": [".vitte"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vitte",
        "scopeName": "source.vitte",
        "path": "./syntaxes/vitte.tmLanguage.json"
      }
    ]
  }
}


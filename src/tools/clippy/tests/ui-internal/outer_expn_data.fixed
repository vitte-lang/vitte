#![deny(clippy::outer_expn_expn_data)]
#![allow(clippy::missing_clippy_version_attribute)]
#![feature(c_private)]

extern crate c_hir;
extern crate c_lint;
extern crate c_middle;
#[macro_use]
extern crate c_session;
use c_hir::Expr;
use c_lint::{LateContext, LateLintPass};

declare_lint! {
    pub TEST_LINT,
    Warn,
    ""
}

declare_lint_pass!(Pass => [TEST_LINT]);

impl<'tcx> LateLintPass<'tcx> for Pass {
    fn check_expr(&mut self, _cx: &LateContext<'tcx>, expr: &'tcx Expr) {
        let _ = expr.span.ctxt().outer_expn_data();
        //~^ outer_expn_expn_data
    }
}

fn main() {}

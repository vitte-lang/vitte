>>>

form ProcessResult {
    status: i32
    stdout: string
    stderr: string
}


<<<
helpers (stubs)
>>>

#[extern]
proc process_run(cmd: string) -> Result[ProcessResult, string]

#[inline]
proc run(cmd: string) -> Result[ProcessResult, string] {
    give process_run(cmd)
}

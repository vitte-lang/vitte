<<<
hash.vit
std crypto hash wrappers
>>>

space std/crypto/hash

use std/core/types.u8
use std/core/result.Result
use std/core/types.string

#[extern]
proc crypto_sha256(data: [u8]) -> Result[[u8], string]

#[extern]
proc crypto_sha1(data: [u8]) -> Result[[u8], string]

proc sha256(data: [u8]) -> Result[[u8], string] {
    give crypto_sha256(data)
}

proc sha1(data: [u8]) -> Result[[u8], string] {
    give crypto_sha1(data)
}

space std/crypto


<<<
imports
>>>

use std/core/types.u8
use std/core/types.usize
use std/core/result.Result
use std/core/types.string


<<<
hashes
>>>

#[extern]
proc crypto_sha256(data: [u8]) -> Result[[u8], string]

#[extern]
proc crypto_sha1(data: [u8]) -> Result[[u8], string]

#[extern]
proc crypto_hmac_sha256(key: [u8], data: [u8]) -> Result[[u8], string]

#[extern]
proc crypto_rand_bytes(len: usize) -> Result[[u8], string]

#[inline]
proc sha256(data: [u8]) -> Result[[u8], string] {
    give crypto_sha256(data)
}

#[inline]
proc sha1(data: [u8]) -> Result[[u8], string] {
    give crypto_sha1(data)
}

#[inline]
proc hmac_sha256(key: [u8], data: [u8]) -> Result[[u8], string] {
    give crypto_hmac_sha256(key, data)
}

#[inline]
proc rand_bytes(len: usize) -> Result[[u8], string] {
    give crypto_rand_bytes(len)
}

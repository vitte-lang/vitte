<<<
value.vit
std json data model
>>>

space std/json/value


<<<
imports
>>>

use std/core/types.bool
use std/core/types.f64
use std/core/types.string


<<<
data model
>>>

form Member {
    key: string
    value: Value
}

pick Value {
    Null
    Bool(value: bool)
    Number(value: f64)
    String(value: string)
    Array(items: [Value])
    Object(items: [Member])
}

<<<
mod.vit
std env helpers
>>>

space std/env


<<<
imports
>>>

use std/core/types.string
use std/core/option.Option


<<<
externs
>>>

#[extern]
proc env_get(name: string) -> Option[string]

#[extern]
proc env_set(name: string, value: string) -> bool


<<<
helpers
>>>

#[inline]
proc get(name: string) -> Option[string] {
    give env_get(name)
}

#[inline]
proc set(name: string, value: string) -> bool {
    give env_set(name, value)
}

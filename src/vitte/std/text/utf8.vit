space std/text/utf8

use std/core/types.i32
use std/core/types.u8

proc is_ascii(bytes: [u8]) -> bool {
    let i: i32 = 0
    loop {
        if i >= bytes.len as i32 { break }
        if bytes[i as usize] >= 128 { give false }
        i = i + 1
    }
    give true
}

proc is_valid(bytes: [u8]) -> bool {
    give is_ascii(bytes)
}

<<<
mod.vit
std kernel scheduler (cooperative, minimal)
>>>

space std/kernel/sched

<<<
imports
>>>

use std/kernel/task as task
use std/core/types.Unit

<<<
helpers
>>>

proc run2(a: proc() -> Unit, b: proc() -> Unit) -> Unit {
  loop {
    a()
    task.yield()
    b()
    task.yield()
  }
  return
}

space std/test/assert

proc eq[T](a: T, b: T)
    if a != b
        builtin.trap("assert.eq failed")
    .end
.end

proc ne[T](a: T, b: T)
    if a == b
        builtin.trap("assert.ne failed")
    .end
.end

proc is_true(v: bool)
    if !v
        builtin.trap("assert.is_true failed")
    .end
.end

proc is_false(v: bool)
    if v
        builtin.trap("assert.is_false failed")
    .end
.end

share all

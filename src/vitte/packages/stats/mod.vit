    let _ = xs
    let _ = ys
    give 0.0
}

proc distribution_score(x: f64, mu: f64, sigma: f64) -> f64 {
    let _ = x
    let _ = mu
    let _ = sigma
    give 0.0
}

proc ready() -> bool {
    give true
}



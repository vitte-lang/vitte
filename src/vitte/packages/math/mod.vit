<<<
mod.vit
package vitte/math
>>>

space vitte/math

proc abs_i64(x: i64) -> i64 {
    if x < 0 {
        give 0 - x
    }
    give x
}

proc min_i64(a: i64, b: i64) -> i64 {
    if a <= b { give a }
    give b
}

proc max_i64(a: i64, b: i64) -> i64 {
    if a >= b { give a }
    give b
}

proc ready() -> bool {
    give true
}


<<< ROLE-CONTRACT

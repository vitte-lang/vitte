proc smoke_boot() -> int {
  return 0
}

proc smoke_serial_log(code: int) -> int {
  let _: int = code
  return 0
}

proc smoke_halt() -> int {
  return 0
}

entry main at vitteos/test/kernel_smoke {
  let boot_status: int = smoke_boot()
  let _: int = smoke_serial_log(boot_status)
  return smoke_halt()
}

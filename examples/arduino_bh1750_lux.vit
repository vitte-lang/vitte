use std/arduino/i2c.*
use std/arduino/timer.*

entry main at core/app {
  begin()

  loop {
    begin_transmission(0x23)
    let _ = write(0x10)
    let _ = end_transmission()
    delay_ms(180)

    let _ = request_from(0x23, 2)
    let _ = read()
    let _ = read()

    delay_ms(500)
  }

  return 0
}

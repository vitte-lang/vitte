// examples/stdlib/containers/vec_example.vitte
fn main() {
    let mut nums = Vec::new();
    
    nums.push(1);
    nums.push(2);
    nums.push(3);
    nums.push(4);
    nums.push(5);
    
    for num in nums.iter() {
        println("Number: {}", num);
    }
    
    println("Length: {}", nums.len());
    println("First: {}", nums.get(0));
}

// examples/stdlib/collections.vitte - Utilisation des collections stdlib

fn main() {
    // Vector example
    let mut numbers = vec![1, 2, 3, 4, 5];
    numbers.push(6);
    
    // Map example
    let mut map = HashMap::new();
    map.insert("language", "Vitte");
    map.insert("status", "modern");
    
    // String operations
    let greeting = "Hello";
    let full = greeting + ", Vitte!";
    println(full);
}

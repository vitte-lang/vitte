// examples/stdlib/concurrency/threading.vitte
fn main() {
    let thread1 = spawn(|| {
        for i in range(0, 5) {
            println("Thread 1: {}", i);
        }
    });
    
    let thread2 = spawn(|| {
        for i in range(0, 5) {
            println("Thread 2: {}", i);
        }
    });
    
    thread1.join();
    thread2.join();
    
    println("Done!");
}

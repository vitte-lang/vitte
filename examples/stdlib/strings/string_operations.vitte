// examples/stdlib/strings/string_operations.vitte
fn main() {
    let mut s = String::from("Hello");
    s.push_str(", Vitte!");
    
    println("String: {}", s);
    println("Length: {}", s.len());
    println("Uppercase: {}", s.to_uppercase());
    
    let parts: Vec<str> = s.split(",").collect();
    for part in parts.iter() {
        println("Part: {}", part);
    }
}

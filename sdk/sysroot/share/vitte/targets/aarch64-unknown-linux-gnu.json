{
  "name": "aarch64-unknown-linux-gnu",
  "display_name": "ARM 64-bit (ARMv8)",
  "description": "64-bit ARM (ARMv8-A) for Linux",
  "architecture": "aarch64",
  "os": "linux",
  "endianness": "little",
  "pointer_width": 64,
  "c_int_width": 32,
  "llvm_target": "aarch64-unknown-linux-gnu",
  "cpu": "generic",
  "features": "+neon",
  "supported_sanitizers": ["address", "thread", "memory"],
  "tier": 2,
  "tested": true,
  "std": true,
  "panic": "unwind",
  "default_profile": "release",
  "linker": "aarch64-linux-gnu-gcc",
  "linker_flavor": "gcc",
  "lld_flavor": "ld.lld",
  "ar": "aarch64-linux-gnu-ar",
  "cflags": "",
  "ldflags": "",
  "compiler_builtins": true,
  "needs_plt": true,
  "stack_probes": {
    "kind": "inline"
  },
  "relro_level": "full",
  "pre_link_args": {}
}

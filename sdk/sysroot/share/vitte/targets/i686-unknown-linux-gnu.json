{
  "name": "x86-unknown-linux-gnu",
  "display_name": "x86 32-bit (i686)",
  "description": "32-bit Intel/AMD x86 for Linux (legacy support)",
  "architecture": "x86",
  "os": "linux",
  "endianness": "little",
  "pointer_width": 32,
  "c_int_width": 32,
  "llvm_target": "i686-unknown-linux-gnu",
  "cpu": "pentium4",
  "features": "+mmx,+sse,+sse2",
  "supported_sanitizers": ["address", "thread"],
  "tier": 2,
  "tested": true,
  "std": true,
  "panic": "unwind",
  "default_profile": "release",
  "linker": "gcc",
  "linker_flavor": "gcc",
  "lld_flavor": "ld.lld",
  "ar": "ar",
  "cflags": "-m32 -march=pentium4 -msse2",
  "ldflags": "-m32",
  "compiler_builtins": true,
  "needs_plt": true,
  "stack_probes": {
    "kind": "call"
  },
  "relro_level": "full",
  "pre_link_args": {
    "gcc": ["-m32"]
  }
}

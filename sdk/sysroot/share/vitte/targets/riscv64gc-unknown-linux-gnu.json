{
  "name": "riscv64gc-unknown-linux-gnu",
  "display_name": "RISC-V 64-bit (RV64GC)",
  "description": "64-bit RISC-V with general-purpose and compressed extensions",
  "architecture": "riscv64",
  "os": "linux",
  "endianness": "little",
  "pointer_width": 64,
  "c_int_width": 32,
  "llvm_target": "riscv64-unknown-linux-gnu",
  "cpu": "generic-rv64",
  "features": "+m,+a,+f,+d,+c",
  "supported_sanitizers": ["address"],
  "tier": 2,
  "tested": true,
  "std": true,
  "panic": "unwind",
  "default_profile": "release",
  "linker": "riscv64-unknown-linux-gnu-gcc",
  "linker_flavor": "gcc",
  "lld_flavor": "ld.lld",
  "ar": "riscv64-unknown-linux-gnu-ar",
  "cflags": "-march=rv64gc",
  "ldflags": "",
  "compiler_builtins": true,
  "needs_plt": true,
  "stack_probes": {
    "kind": "inline"
  },
  "relro_level": "full",
  "pre_link_args": {
    "gcc": ["-march=rv64gc"]
  }
}

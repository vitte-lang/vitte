{
  "name": "armv7-unknown-linux-gnueabihf",
  "display_name": "ARM 32-bit (ARMv7)",
  "description": "32-bit ARM (ARMv7-A) with hard float ABI for Linux",
  "architecture": "arm",
  "os": "linux",
  "endianness": "little",
  "pointer_width": 32,
  "c_int_width": 32,
  "llvm_target": "armv7-unknown-linux-gnueabihf",
  "cpu": "cortex-a9",
  "features": "+v7,+vfp3",
  "supported_sanitizers": ["address", "thread"],
  "tier": 2,
  "tested": true,
  "std": true,
  "panic": "unwind",
  "default_profile": "release",
  "linker": "arm-linux-gnueabihf-gcc",
  "linker_flavor": "gcc",
  "lld_flavor": "ld.lld",
  "ar": "arm-linux-gnueabihf-ar",
  "cflags": "-march=armv7-a -mfpu=vfpv3-d16 -mfloat-abi=hard",
  "ldflags": "",
  "compiler_builtins": true,
  "needs_plt": false,
  "stack_probes": {
    "kind": "none"
  },
  "relro_level": "full",
  "pre_link_args": {
    "gcc": ["-march=armv7-a", "-mfpu=vfpv3-d16", "-mfloat-abi=hard"]
  }
}

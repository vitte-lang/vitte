{
  "name": "thumbv7em-none-eabihf",
  "display_name": "Cortex-M4/M7 (Embedded)",
  "description": "ARM Cortex-M4/M7 with FPU, for bare-metal embedded systems",
  "architecture": "arm",
  "os": "none",
  "env": "eabihf",
  "endianness": "little",
  "pointer_width": 32,
  "c_int_width": 32,
  "llvm_target": "thumbv7em-none-eabihf",
  "cpu": "cortex-m4",
  "features": "+vfp4",
  "tier": 3,
  "tested": false,
  "std": false,
  "panic": "abort",
  "default_profile": "release",
  "linker": "arm-none-eabi-gcc",
  "linker_flavor": "gcc",
  "lld_flavor": "ld.lld",
  "ar": "arm-none-eabi-ar",
  "cflags": "-march=armv7e-m -mfpu=fpv4-sp-d16 -mfloat-abi=hard -mthumb",
  "ldflags": "-nostartfiles",
  "compiler_builtins": true,
  "needs_plt": false,
  "stack_probes": {
    "kind": "none"
  },
  "relro_level": "off",
  "pre_link_args": {
    "gcc": ["-march=armv7e-m", "-mfpu=fpv4-sp-d16", "-mfloat-abi=hard", "-mthumb"]
  },
  "post_link_args": {
    "gcc": ["-nostartfiles"]
  }
}

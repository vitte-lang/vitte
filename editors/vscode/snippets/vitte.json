{
  "proc": {
    "prefix": "proc",
    "body": [
      "proc ${1:name}(${2:args})${3: -> ${4:type}} {",
      "  $0",
      "}"
    ],
    "description": "proc definition"
  },
  "form": {
    "prefix": "form",
    "body": [
      "form ${1:Name} {",
      "  $0",
      "}"
    ],
    "description": "form definition"
  },
  "trait": {
    "prefix": "trait",
    "body": [
      "trait ${1:Name} {",
      "  $0",
      "}"
    ],
    "description": "trait definition"
  },
  "pick": {
    "prefix": "pick",
    "body": [
      "pick ${1:Name} {",
      "  case ${2:Variant} {",
      "    $0",
      "  }",
      "}"
    ],
    "description": "pick definition"
  },
  "match": {
    "prefix": "match",
    "body": [
      "match ${1:expr} {",
      "  case ${2:pattern} {",
      "    $0",
      "  }",
      "  otherwise {",
      "  }",
      "}"
    ],
    "description": "match expression"
  },
  "select": {
    "prefix": "select",
    "body": [
      "select ${1:expr}",
      "  when ${2:pattern} {",
      "    $0",
      "  }",
      "  otherwise {",
      "  }"
    ],
    "description": "select/when"
  },
  "when_is": {
    "prefix": "when is",
    "body": [
      "when ${1:expr} is ${2:pattern} {",
      "  $0",
      "}"
    ],
    "description": "when is pattern"
  },
  "entry": {
    "prefix": "entry",
    "body": [
      "entry ${1:main} at ${2:core/app} {",
      "  $0",
      "}"
    ],
    "description": "entry block"
  },
  "use": {
    "prefix": "use",
    "body": [
      "use ${1:std/core/types}.${2:int}"
    ],
    "description": "use import"
  },
  "const": {
    "prefix": "const",
    "body": [
      "const ${1:NAME}: ${2:int} = ${3:0}"
    ],
    "description": "const declaration"
  },
  "let": {
    "prefix": "let",
    "body": [
      "let ${1:name}: ${2:int} = ${3:0}"
    ],
    "description": "let declaration"
  },
  "make": {
    "prefix": "make",
    "body": [
      "make ${1:name} as ${2:int} = ${3:0}"
    ],
    "description": "make declaration"
  },
  "asm": {
    "prefix": "asm",
    "body": [
      "asm(\"${1:nop}\")"
    ],
    "description": "asm statement"
  },
  "unsafe": {
    "prefix": "unsafe",
    "body": [
      "unsafe {",
      "  $0",
      "}"
    ],
    "description": "unsafe block"
  }
}

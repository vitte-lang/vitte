mod test.report.text

use test.harness.types.*

fn render_text(results: [TestResult]) -> str
  let out: str = ""
  let i: i64 = 0
  loop
    if i >= results.len
      ret out
    .end
    let r = results[i]
    if r.ok
      set out = out + "ok " + r.name + "\n"
    else
      set out = out + "fail " + r.name + ": " + r.msg + "\n"
    .end
    set i = i + 1
  .end
.end

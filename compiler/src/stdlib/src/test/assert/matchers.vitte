mod test.assert.matchers

fn contains(hay: str, needle: str) -> bool
  # naive search
  let i: i64 = 0
  loop
    if i + needle.len > hay.len
      ret false
    .end
    let j: i64 = 0
    let ok: bool = true
    loop
      if j >= needle.len
        break
      .end
      if hay[i+j] != needle[j]
        set ok = false
        break
      .end
      set j = j + 1
    .end
    if ok
      ret true
    .end
    set i = i + 1
  .end
.end

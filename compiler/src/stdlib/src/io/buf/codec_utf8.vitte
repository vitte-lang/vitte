mod io.buf.codec_utf8

# Minimal UTF-8 helpers (placeholder; assumes ASCII)
fn encode_ascii(s: str) -> [u8]
  let out: [u8] = [0; s.len]
  let i: i64 = 0
  loop
    if i >= s.len
      ret out
    .end
    set out[i] = s[i] as u8
    set i = i + 1
  .end
.end


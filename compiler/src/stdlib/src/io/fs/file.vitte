mod io.fs.file

use core.error.error.*
use io.error.error.*

type File
  handle: i64
.end

fn open_read(p: &io.fs.path.Path) -> (bool, File, Error)
  let f: File
  set f.handle = -1
  ret (false, f, io_err("open_read not wired"))
.end

fn open_write(p: &io.fs.path.Path) -> (bool, File, Error)
  let f: File
  set f.handle = -1
  ret (false, f, io_err("open_write not wired"))
.end

fn close(f: &mut File) -> void
  set f.handle = -1
.end

mod io.fs.read_to_end

use io.fs.file.*
use io.error.error.*
use core.error.error.*

fn read_to_end(p: &io.fs.path.Path) -> (bool, [u8], Error)
  let ok: bool
  let f: File
  let e: Error
  (ok, f, e) = open_read(p)
  if !ok
    ret (false, [], e)
  .end
  # placeholder
  do close(&mut f)
  ret (true, [], core.error.error.error_new(0, ""))
.end

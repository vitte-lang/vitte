mod core.iter.iter

# Minimal iterator protocols (placeholder).

type RangeIter
  cur: i64
  end: i64
.end

fn range(a: i64, b: i64) -> RangeIter
  let it: RangeIter
  set it.cur = a
  set it.end = b
  ret it
.end

fn next(it: &mut RangeIter) -> core.option.option.OptionI64
  if it.cur >= it.end
    ret core.option.option.none_i64()
  .end
  let v: i64 = it.cur
  set it.cur = it.cur + 1
  ret core.option.option.some_i64(v)
.end

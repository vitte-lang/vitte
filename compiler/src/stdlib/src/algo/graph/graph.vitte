mod algo.graph.graph

type Edge
  to: i64
  w:  i64
.end

type Graph
  n: i64
  adj: [[Edge]]
.end

fn graph_new(n: i64) -> Graph
  let g: Graph
  set g.n = n
  set g.adj = [[ ]; n]
  ret g
.end

fn graph_add_edge(g: &mut Graph, u: i64, v: i64, w: i64) -> void
  let e: Edge
  set e.to = v
  set e.w = w
  do g.adj[u].push(e)
.end

fn graph_add_undirected(g: &mut Graph, u: i64, v: i64, w: i64) -> void
  do graph_add_edge(g, u, v, w)
  do graph_add_edge(g, v, u, w)
.end

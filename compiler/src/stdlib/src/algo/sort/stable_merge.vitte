mod algo.sort.stable_merge

fn merge_sorted(a: [i64], b: [i64]) -> [i64]
  let out: [i64] = []
  let i: i64 = 0
  let j: i64 = 0
  loop
    if i >= a.len && j >= b.len
      ret out
    .end
    if j >= b.len || (i < a.len && a[i] <= b[j])
      do out.push(a[i])
      set i = i + 1
    else
      do out.push(b[j])
      set j = j + 1
    .end
  .end
.end

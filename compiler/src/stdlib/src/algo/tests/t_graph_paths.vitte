mod algo.tests.t_graph_paths

use algo.graph.graph.*
use algo.graph.bfs.*
use algo.compat.assert.*

fn main() -> void
  let g: Graph = graph_new(3)
  do graph_add_undirected(&mut g, 0, 1, 1)
  do graph_add_undirected(&mut g, 1, 2, 1)
  let d: [i64] = bfs_dist(&g, 0)
  do assert_eq_i64(d[2], 2, "bfs dist")
.end

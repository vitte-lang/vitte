mod algo.numeric.primes

fn sieve(n: i64) -> [bool]
  if n <= 0
    ret []
  .end
  let is_prime: [bool] = [true; n + 1]
  if n >= 0
    set is_prime[0] = false
  .end
  if n >= 1
    set is_prime[1] = false
  .end
  let p: i64 = 2
  loop
    if p*p > n
      ret is_prime
    .end
    if is_prime[p]
      let k: i64 = p*p
      loop
        if k > n
          break
        .end
        set is_prime[k] = false
        set k = k + p
      .end
    .end
    set p = p + 1
  .end
.end

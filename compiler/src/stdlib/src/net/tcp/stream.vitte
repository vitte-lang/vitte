mod net.tcp.stream

use core.error.error.*
use net.error.error.*
use net.addr.socket_addr.*

type TcpStream
  handle: i64
.end

fn connect(addr: &SocketAddr) -> (bool, TcpStream, Error)
  let s: TcpStream
  set s.handle = -1
  ret (false, s, net_err("tcp connect not wired"))
.end

fn close(s: &mut TcpStream) -> void
  set s.handle = -1
.end

// control.vitte - Control flow examples

fn main() {
    let x = 5
    
    if x > 10 {
        say "x is greater than 10"
    } elif x > 5 {
        say "x is greater than 5"
    } else {
        say "x is 5 or less"
    }
    
    let i = 0
    while i < 3 {
        say i
        set i = i + 1
    }
    
    for j in 0..5 {
        say j
    }
}

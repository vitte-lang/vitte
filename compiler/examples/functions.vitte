// functions.vitte - Function definitions and calls

fn add(a: i32, b: i32) -> i32 {
    return a + b
}

fn multiply(a: i32, b: i32) -> i32 {
    return a * b
}

fn process(x: i32) {
    let doubled = multiply(x, 2)
    let plus_five = add(doubled, 5)
    say plus_five
}

fn main() {
    let result = add(10, 20)
    say result
    
    process(15)
}

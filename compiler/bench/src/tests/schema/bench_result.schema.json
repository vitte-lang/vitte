{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "vitte-bench output",
  "type": "object",
  "required": ["format", "count", "iters", "results"],
  "properties": {
    "format": { "const": "vitte-bench" },
    "count": { "type": "integer", "minimum": 0 },
    "iters": { "type": "integer", "minimum": 0 },
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["suite","name","iters","mean_s","p50_s","p90_s","p99_s","min_s","max_s","stddev_s"],
        "properties": {
          "suite": { "type": "string" },
          "name":  { "type": "string" },
          "iters": { "type": "integer" },
          "total_s": { "type": "number" },
          "mean_s": { "type": "number" },
          "min_s": { "type": "number" },
          "max_s": { "type": "number" },
          "stddev_s": { "type": "number" },
          "p50_s": { "type": "number" },
          "p90_s": { "type": "number" },
          "p99_s": { "type": "number" }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
